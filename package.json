{
  "name": "boilerplate-django-vue",
  "version": "1.0.0",
  "description": "A boilerplate to help skip all that boring stuff of installs and configurations",
  "main": "app.js",
  "scripts": {
    "build": "webpack --mode=production --config webpack.config.js --progress --colors",
    "start": "webpack --mode=development --config webpack.config.js --watch"
  },
  "keywords": [
    "django",
    "webpack",
    "vue",
    "rest",
    "framework"
  ],
  "author": "Ayron Felipe",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.19.0",
    "vue": "^2.6.10",
    "vue-router": "^3.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.7.5",
    "@babel/preset-env": "^7.7.5",
    "babel-loader": "^8.0.6",
    "file-loader": "^5.0.2",
    "vue-loader": "^15.7.2",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.12"
  }
}
